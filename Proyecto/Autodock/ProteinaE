npts 122 88 88                       # num.grid points in xyz
gridfld ProteinaE_model1.maps.fld    # grid_data_file
spacing 0.375                        # spacing(A)
receptor_types A C HD N OA           # receptor atom types
ligand_types A C HD OA N             # ligand atom types
receptor ProteinaE_model1.pdbqt      # macromolecule
gridcenter 24.894 0.015 0.023        # xyz-coordinates or auto
smooth 0.5                           # store minimum energy w/in rad(A)
map ProteinaE_model1.A.map           # atom-specific affinity map
map ProteinaE_model1.C.map           # atom-specific affinity map
map ProteinaE_model1.HD.map          # atom-specific affinity map
map ProteinaE_model1.OA.map          # atom-specific affinity map
map ProteinaE_model1.N.map           # atom-specific affinity map
elecmap ProteinaE_model1.e.map       # electrostatic potential map
dsolvmap ProteinaE_model1.d.map              # desolvation potential map
dielectric -0.1465                   # <0, AD4 distance-dep.diel;>0, constant
